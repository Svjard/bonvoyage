{
  "name": "bonvoyage-frontend",
  "description": "Frontend UI and logic for bonvoyage",
  "version": "0.0.1",
  "author": "Marc Fisher",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Svjard/bonvoyage"
  },
  "main": "client/scripts/main.js",
  "scripts": {
    "build": "./node_modules/.bin/gulp prod",
    "clean": "rimraf dist",
    "lint": "./node_modules/.bin/gulp lint",
    "karma:all": "./node_modules/.bin/karma start --browsers=PhantomJS,Chrome,Firefox",
    "karma:chrome": "./node_modules/.bin/karma start --browsers=Chrome",
    "karma:firefox": "./node_modules/.bin/karma start --browsers=Firefox",
    "karma:ie": "./node_modules/.bin/karma start --browsers=IE",
    "package:check": "./node_modules/.bin/ncu",
    "package:dev": "./node_modules/.bin/ncu -p -u",
    "package:prod": "./node_modules/.bin/ncu -u",
    "package:purge": "rm -rf node_modules",
    "package:reinstall": "rm -rf ./npm-shrinkwrap.json && npm run package:purge && npm install && npm run package:deploy",
    "package:deploy": "npm shrinkwrap",
    "package:upgrade": "./node_modules/.bin/ncu -u && npm run package:deploy",
    "packages": "npm list --depth=0",
    "postpublish": "git push origin master --follow-tags",
    "preversion": "[[ -z $(git status --porcelain) ]] && npm run production && git add -A dist && git commit -m 'Build lib'",
    "start": "./node_modules/.bin/gulp dev",
    "test": "./node_modules/.bin/gulp test",
    "test:watch": "npm test -- --watch",
    "test:ui": "./node_modules/protractor/bin/protractor protractor.conf.js"
  },
  "dependencies": {
    "async": "^1.5.2",
    "blacklist": "^1.1.2",
    "chance": "*",
    "classnames": "^2.2.3",
    "d3": "^3.5.13",
    "dom-helpers": "^2.4.0",
    "fast-apply": "0.0.3",
    "fastclick": "^1.0.6",
    "invariant": "^2.1.2",
    "keycode": "^2.1.0",
    "lodash": "^4.0.0",
    "lodash-compat": "^3.10.1",
    "moment": "^2.11.1",
    "object-assign": "^4.0.1",
    "qs": "^6.0.1",
    "react": "^0.14.6",
    "react-addons-css-transition-group": "^0.14.6",
    "react-addons-update": "^0.14.8",
    "react-dom": "^0.14.6",
    "react-ga": "^1.3.3",
    "react-overlays": "^0.5.2",
    "react-prop-types": "^0.3.0",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0-rc5",
    "redux": "^3.0.5",
    "redux-promise": "^0.5.1",
    "redux-thunk": "^1.0.3",
    "superagent": "^1.7.0",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.4.5",
    "babel-eslint": "^6.0.0-beta.6",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-plugin-typecheck": "^3.6.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "browser-sync": "^2.7.13",
    "browserify": "^12.0.1",
    "cookies-js": "^1.2.2",
    "debowerify": "^1.4.1",
    "del": "^2.2.0",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^3.1.0",
    "gulp-babel-istanbul": "^1.0.0",
    "gulp-eslint": "^2.0.0",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^2.4.0",
    "gulp-mocha": "^2.2.0",
    "gulp-nodemon": "^2.0.6",
    "gulp-notify": "^2.2.0",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.2.0",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-streamify": "^1.0.2",
    "gulp-uglify": "^1.5.3",
    "gulp-util": "^3.0.7",
    "humps": "^1.0.0",
    "jsdom": "^8.2.0",
    "mocha": "^2.4.5",
    "react-addons-test-utils": "^0.14.7",
    "run-sequence": "^1.1.5",
    "should": "^8.3.0",
    "sinon": "^1.17.3",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.7.0",
    "yargs": "^3.27.0"
  },
  "engines": {
    "node": ">=4.1 <6"
  },
  "devEngines": {
    "node": ">=4.1 <6",
    "npm": ">=3.1 <5"
  }
}
